    <style name="my_text_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textColor">@color/title_text_color</item>
        <item name="android:textSize">@dimen/title_textsize</item>
        <item name="android:paddingLeft">10dp</item>
    </style>

    <style name="my_right_arrow_style">
        <item name="android:layout_width">8.4dp</item>
        <item name="android:layout_height">13.2dp</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginRight">15dp</item>
        <item name="android:background">@drawable/right_arrow_ico</item>
        <item name="android:layout_alignParentRight">true</item>
    </style>

    <style name="divider_line_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/divider_line_height</item>
        <item name="android:background">@color/line_color</item>
    </style>

    <style name="my_line_style" parent="divider_line_style">
        <item name="android:layout_marginLeft">63dp</item>
    </style>



</resources>